
@article{sell_wahrnehmung_2020,
	title = {Wahrnehmung und Wirklichkeit in der Volkswirtschaftslehre},
	volume = {71},
	issn = {2366-0481},
	url = {https://www.degruyter.com/document/doi/10.1515/ordo-2021-0003/html},
	doi = {10.1515/ordo-2021-0003},
	abstract = {Zusammenfassung Wahrnehmung und Wirklichkeit fallen in wirtschaftlichen Zusammenhängen häufig auseinander. In diesem Beitrag wird gezeigt, wie Volkswirte – auch im Vergleich zu anderen Disziplinen – mit diesen beiden Themen umgehen. Da (umfassend) rationale Wahrnehmungen, analog zu den rationalen Erwartungen, höchst selten anzutreffen sind, bedarf es an Erklärungsansätzen, die, als Teil der Behavioral Economics, Abweichungen davon befriedigend erklären können. Die Prospect Theory erweist sich dabei als leistungsfähiger Ansatz. Mit seiner Hilfe kann die Koexistenz von Ungleichheits- wie Gleichheitsaversion in der personellen Einkommensverteilung gut begründet werden. Die bisherige Empirie dazu vermag zwar noch nicht zu überzeugen, was aber mit dem noch fehlenden Kriterienkatalog für die Bildung/Abgrenzung von Ländersamples – analog zu den Konvergenztestes in der empirischen Wachstumsforschung – erklärt werden kann.},
	pages = {63--89},
	number = {1},
	journaltitle = {{ORDO}},
	author = {Sell, Friedrich L.},
	urldate = {2022-09-24},
	date = {2020-04-01},
	langid = {german},
	note = {Publisher: De Gruyter Oldenbourg},
	keywords = {Equilibrium in Income Distribution, Personal Income Distribution, Prospect Theory, Rational Perceptions and Errors, Self-perception of Economics},
	file = {Full Text PDF:C\:\\Users\\janvo\\Zotero\\storage\\K43WQUHM\\Sell - 2020 - Wahrnehmung und Wirklichkeit in der Volkswirtschaf.pdf:application/pdf},
